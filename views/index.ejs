<%- include('partials/_head') %>
<main>
  <!-- Carousel -->

  <div
    id="carouselExampleCaptions"
    class="carousel slide"
    data-bs-ride="carousel"
  >
    <div class="carousel-indicators">
      <button
        type="button"
        data-bs-target="#carouselExampleCaptions"
        data-bs-slide-to="0"
        class="carousel-bottom-btn active"
        aria-current="true"
        aria-label="Slide 1"
      ></button>
      <button
        type="button"
        data-bs-target="#carouselExampleCaptions"
        data-bs-slide-to="1"
        class="carousel-bottom-btn"
        aria-label="Slide 2"
      ></button>
      <button
        type="button"
        data-bs-target="#carouselExampleCaptions"
        data-bs-slide-to="2"
        class="carousel-bottom-btn"
        aria-label="Slide 3"
      ></button>
    </div>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img
          src="https://blog.coldwellbankerluxury.com/wp-content/uploads/2016/04/Amalfi_Palisades_BLOG.jpg"
          class="demo-homes carousel-home-1"
          alt="..."
        />
        <div class="carousel-caption d-none d-md-block">
          <a href="/properties"><button class="view-properties btn btn-secondary btn-lg">View Properties Now</button></a>
        </div>
      </div>
      <div class="carousel-item">
        <img
          src="https://i.pinimg.com/originals/45/f4/61/45f461e5faddd823a169852c8b415c5a.jpg"
          class="demo-homes carousel-home-2"
          alt="..."
        />
        <div class="carousel-caption d-md-block">
          <a href="/properties"><button class="view-properties btn btn-secondary btn-lg">View Properties Now</button></a>
        </div>
      </div>
      <div class="carousel-item">
        <img
          src="images/pacific-palisades.jpg"
          class="demo-homes carousel-home-3"
          alt="..."
        />
        <div class="carousel-caption d-none d-md-block">
          <a href="/properties"><button class="view-properties btn btn-secondary btn-lg">View Properties Now</button></a>
        </div>
      </div>
    </div>
    <div class="previous-button-container">
      <button
        class="carousel-control-prev"
        type="button"
        data-bs-target="#carouselExampleCaptions"
        data-bs-slide="prev"
      >
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
    </div>
    <button
      class="carousel-control-next"
      type="button"
      data-bs-target="#carouselExampleCaptions"
      data-bs-slide="next"
    >
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>

